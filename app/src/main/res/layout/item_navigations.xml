<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class ="NewsBinding">
        <variable
            name="NewsView"
            type="ua.com.dynamicrecyclerview.drawingreadingswelder.viewmodel.NavagationsSceneMVVM"/>

    </data>


<LinearLayout
    xmlns:tooals="http://schemas.android.com/tools"
    android:id="@+id/linear"
    android:onClick="@{NewsView::onClick}"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/title"
        android:text="@{NewsView.titleNavigation}"
        android:paddingLeft="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:textSize="22sp"
        tooals:text="Hello" />

</LinearLayout>

</layout>